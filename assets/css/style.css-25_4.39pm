
body {
	background-image: url("../images/brickwall_@2X.png");
}


.divBanner {
	width: 100%;
	height: 80px;
	border-bottom: 1px solid #cccccc;
	border-top: 1px solid #cccccc;
	border-color: Gray;
	position: relative;
	margin: 0 auto: ;
	background-color: #ffffff;
}

.divNameHeading {
	width: 250px;
	height: 80px;
	background-color: #4aaaa5;
	position: absolute;
	top: 9px;
	left: 160px;
	text-align: center;
	z-index: 2;	
}


.divNameHeading h1 {
	color: White;
	font-size: 30px;
}

.divNavigationBar {
	width: 450px; 
	height: 80px;
	position: absolute;
	top: 35px;
	right: 310px;
}

.divNavigationBar ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    background-color: #ffffff;
    font-size: 20px;
}

.divNavigationBar li {
	display: inline;
	padding-right: 10px;
	padding-left: 10px;
	border-left: solid 1px Gray;

}

.divNavigationBar li a {
	display: inline;
	text-decoration: none;
	color: #777777;
}

.divTotalContent {
	width: 960px;
	height: 650px;
	margin-left: 152px;
}

.divMainSection {
	width: 648px;
	height: 650px;
	margin-top: 35px;
	background-color: #ffffff;
	border: solid 1px #cccccc;
	float: left;
	border: dotted 1px Gray;
	margin-right: 40px;

}



.divMainSection	 h1 {
	color: #4aaaa5;
	font-size: 30px;
	border-bottom: dotted 1px Gray;
	padding-top: 10px;
	padding-bottom: 25px;
	margin-left: 25px;
	margin-right: 25px;
}




.divMainSection h2 {
	color: Gray;	
	font-size: 20px;
	margin-left: 25px;
	margin-right: 25px;
}


.divMainSection img {
	width: 200px;
	height: 200px;
	border: dotted 1px #dddddd;
	float: left;
	margin-left: 25px;
	margin-right: 25px;
}

.divMainSection p {
	font-size: 20px;
	color: #777777;
	line-height: 1.5;
	margin-left: 25px;
	margin-right: 25px;
}

.divMainSection form {
	margin-left: 25px;
}

input[type=text], select {
    width: 96%;
    padding: 12px 20px;
    margin: 8px 0;
    display: inline-block;
    border: 1px solid #dddddd;
    box-sizing: border-box;
    color: #777777;
    font-size: 16px;
}

input[type=submit] {
    width: 20%;
    padding: 12px 20px;
    margin: 8px 0;
    display: inline-block;
    border: 1px solid #dddddd;
    box-sizing: border-box;
    background-color: #4aaaa5;	
    color: #ffffff;
    font-size: 18px;
}

.divFormMessageBox {
	height: 220px;
}




.divSideBar {
	width: 268px;
	height: 200px;
	margin-top: 35px;
	background-color: #ffffff;
	border: solid 1px #cccccc;
	float: left;
}

.divSideBar h2 {
	color: #4aaaa5;
	font-size: 25px;
	text-align: center;
	margin-top: 25px;
	padding-bottom: 1px;

}


.imgContainer {
	width: 260px;
	height: 75px;
	margin: 0 auto;
	padding-top: 25px;
	border-top: solid 1px Gray;
}


.imgContainerGithub {
	float: left;
	margin-left: 30px;
}

.imgContainerLinkedin {
	float: left;
	margin-left: 10px;
	margin-right: 10px;
}

.imgContainerStackoverflow {
	float: right;
	margin-right: 30px;
	margin-top: 35px;
}




.divFooter1 {
	background-color: #4aaaa5;
	width:100%;
	height: 10px;
	margin-top: 100px;
	
}

.divFooter2 {
	background-color: #666666;
	width: 100%;
	height: 50px;
	text-align: center;
	padding-top: 45px;
	color: #AEB6BF;
	
}


